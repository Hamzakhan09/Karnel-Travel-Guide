@model IEnumerable<Karnel_Travel_Guide.Models.Rest_Cat>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ResCat_Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ResCat_Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.RestCat_Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.RestCat_Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.RestCat_Id })
        </td>
    </tr>
}

</table>
